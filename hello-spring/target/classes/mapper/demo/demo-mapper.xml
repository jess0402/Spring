<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="demo">

	<insert id="insertDev">
		insert into
			dev
		values(
			seq_dev_no.nextval,
			#{name},
			#{career},
			#{email},
			#{gender},
			#{lang},
			default)
	</insert>
	
	<select id="selectDevList" resultMap="devMap">
		select
			*
		from
			dev
		order by
			no desc
	</select>
	<resultMap type="dev" id="devMap">
		<!-- <id column="no" property="no" /> <- 이렇게 이름이 똑같은것들은 비워놔도 됨. -->
	</resultMap>
	
	<delete id="deleteDev">
		delete from dev
		where no = #{no}
	</delete>
</mapper>